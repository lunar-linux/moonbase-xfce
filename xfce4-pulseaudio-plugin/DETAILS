          MODULE=xfce4-pulseaudio-plugin
         VERSION=0.4.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=http://archive.xfce.org/src/panel-plugins/$MODULE/${VERSION%.*}/
      SOURCE_VFY=sha256:6ca88314dbac3e24c0e1bfc593fad6edb66319766be62e8256c81b0314f049f0
